<h1>Links</h1>
  <%if current_user %>
  Welcome <%= current_user.email %>
  <%end%>

<ul id='links'>
  <% @links.each do |link| %>
  <li>
    Title: <%= link.title %>
    URL: <%= link.url %>
  </li>
    <% end %>
</ul>


<form action="/add_link" method="get">
  <input type="submit" value="Add link">
</form>
